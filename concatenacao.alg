algoritmo "Concatenação"
// Função :
// Autor :
// Data : 7/27/2022
// Seção de Declarações 
var
   nome: caractere
   numero: real

inicio
// Seção de Comandos 
   escreval("Digite o nome: ")
   leia(nome)
   escreval("Digite o número: ")
   leia(numero)
   
   escreval(nome, " :", numero, 15)

fimalgoritmo
