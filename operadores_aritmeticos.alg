algoritmo "Operadores Aritméticos"
// Função :
// Autor :
// Data : 7/27/2022
// Seção de Declarações 
var
   resultado, valor1, valor2, valor3: real
     
inicio
// Seção de Comandos
   valor1 := 10
   valor2 := 10
   valor3 := 10
   resultado := ((valor1 + valor2 + valor2) / valor3) % 2
   escreval("Resultado: ", resultado)
   
fimalgoritmo
