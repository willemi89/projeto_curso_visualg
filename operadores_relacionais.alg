algoritmo "Operadores Relacionais"
// Função :
// Autor :
// Data : 7/27/2022
// Seção de Declarações 
var
   nota1, nota2: real

inicio
// Seção de Comandos 
   nota1 := 10
   nota2 := 15
   
   se(nota1 = nota2) entao
     escreval("Nota1 é diferente")
   senao
     escreval("Nota1 é igual")
   fimse

fimalgoritmo
